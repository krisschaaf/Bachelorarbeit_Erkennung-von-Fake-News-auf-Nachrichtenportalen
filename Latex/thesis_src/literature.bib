	journal = {SN Computer Science},
	number = {5},
	pages = {595},
	title = {Advancing Software Vulnerability Scoring: A Statistical Approach with Machine Learning Techniques and GridSearchCV Parameter Tuning},
	url = {https://doi.org/10.1007/s42979-024-02942-x},
	volume = {5},
	year = {2024},
